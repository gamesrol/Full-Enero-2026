generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mongodb"
}

model users {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  v        Int    @map("__v")
  email    String @unique(map: "email_1")
  name     String
  password String
}
